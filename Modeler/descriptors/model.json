{
  "name": "modelExtension",
  "prefix": "model",
  "uri": "http://model",
  "xml": {
    "tagAlias": "lowerCase"
  },
  "types": [
    {
      "name": "CustomProcess",
      "extends": ["bpmn:Process"],
      "properties": [
        {
          "name": "instance",
          "isAttr": true,
          "type": "Integer"
        },
        {
          "name": "frequency",
          "isAttr": true,
          "type": "Integer"
        },
        {
          "name": "userWithoutRole",
          "isAttr": false,
          "isMany": true,
          "type": "String"
        },
        {
          "name": "userWithRole",
          "isAttr": false,
          "isMany": true,
          "type": "KeyValuePair"
        },
        {
          "name": "security",
          "isAttr": true,
          "type": "Boolean"
        }
      ]
    },
    {
      "name": "KeyValuePair",
      "properties": [
        {
          "name": "key",
          "type": "String"
        },
        {
          "name": "value",
          "type": "String"
        }
      ]
    }
  ]
}
